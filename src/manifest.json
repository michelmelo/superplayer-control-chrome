{
  "manifest_version": 2,

  "name": "Superplayer Control",
  "description": "Extension to control Superplayer",
  "version": "1.0",
  "permissions": [
    "tabs", "https://www.superplayer.fm/*", "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_keys": {
        "mac": "Control+Shift+P"
      }
    }
  }
}
